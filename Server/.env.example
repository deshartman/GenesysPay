# Environment Configuration Template
# Copy this file to .env.dev or .env.prod and fill in your actual values

# Twilio Account Credentials
ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
AUTH_TOKEN=your_auth_token_here
AU_AUTH_TOKEN=your_au_auth_token_here

# Variables for function "pv"
# ---
SERVER_URL=your-service-name.twil.io
SIP_DOMAIN_URI=your-domain.sip.twilio.com

# Variables for function "sync"
# IMPORTANT: Sync only works in US1 region.
PAY_SYNC_SERVICE_NAME=AgentAssistedPaySync
PAY_SYNC_SERVICE_SID=ISxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
SYNC_PAY_MAP_NAME=payMap
SYNC_UUI_MAP_NAME=uuiMap


# Variables for function "aap"
# ---
PAYMENT_CONNECTOR=GenericPay_in_Functions
INCLUDE_CVC=true
INCLUDE_POSTAL_CODE=false
PAYMENT_CAPTURE_ORDER="payment-card-number,security-code,expiration-date"

# Twilio API Keys (US1)
# ---
API_KEY=SKxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
API_SECRET=your_api_secret_here

# Twilio API Keys (AU1)
API-KEY_AU=SKxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
API_SECRET_AU=your_api_secret_here

# Twilio API Keys (IE1)
API-KEY_IE=SKxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
API_SECRET_IE=your_api_secret_here

